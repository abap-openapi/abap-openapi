{
  "openapi": "3.0.0",
  "info": {
    "title": "return structured array",
    "version": "1"
  },
  "paths": {
    "/array": {
      "post": {
        "responses": {
          "200": {
            "description": "foo",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Something"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Something": {
        "type": "object",
        "properties": {
          "subsomething": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "key": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}