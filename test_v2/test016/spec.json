{
  "openapi": "3.0.3",
  "info": {
    "title": "Swagger Petstore - OpenAPI 3.0",
    "version": "1.0.11"
  },
  "paths": {
    "/createDog": {
      "post": {
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "detail": {
                    "allOf": [
                      {
                        "$ref": "#/components/schemas/Dog"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Created"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Dog": {
        "type": "object",
        "properties": {
          "bark": {
            "type": "boolean"
          },
          "breed": {
            "type": "string",
            "enum": ["Dingo", "Husky", "Retriever", "Shepherd"]
          }
        }
      }
    }
  }
}
